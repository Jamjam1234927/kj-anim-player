-- Scripts:

local function DQUGDEJ_fake_script() -- KJUlt.LocalScript 
	local script = Instance.new('LocalScript', KJUlt)

	local Humanoid = game.Players.LocalPlayer.Character.Humanoid
	local Animator = Humanoid and Humanoid:FindFirstChild('Animator')
	if Animator then
		Animator:Destroy()
	end
	
	
	local anim = Instance.new('Animation',script.Parent)
	anim.AnimationId = "rbxassetid://18445236460"
	
	ClientAnimator = Humanoid:FindFirstChild('Animator')
	
	if not Humanoid:FindFirstChild('Animator') then
		ClientAnimator = Instance.new('Animator',Humanoid)
	end
	
	
	script.Parent.MouseButton1Click:Connect(function()
		local KJUlt = ClientAnimator:LoadAnimation(anim)
		KJUlt:Play()
	
		local Sound = Instance.new('Sound')
		Sound.SoundId = "rbxassetid://18445228136"
		Sound.Parent = Humanoid.Parent
		Sound:Play()
		game.Debris:AddItem(Sound,Sound.TimeLength+.5)
	
	end)
end
coroutine.wrap(DQUGDEJ_fake_script)()
local function YNRLSZK_fake_script() -- DropkickLanded.LocalScript 
	local script = Instance.new('LocalScript', DropkickLanded)

	local Humanoid = game.Players.LocalPlayer.Character.Humanoid
	local Animator = Humanoid and Humanoid:FindFirstChild('Animator')
	if Animator then
		Animator:Destroy()
	end
	
	
	local anim = Instance.new('Animation',script.Parent)
	anim.AnimationId = "rbxassetid://17420452843"
	ClientAnimator = Humanoid:FindFirstChild('Animator')
	if not Humanoid:FindFirstChild('Animator') then
		ClientAnimator = Instance.new('Animator',Humanoid)
	end
	
	
	script.Parent.MouseButton1Click:Connect(function()
		local KJUlt = ClientAnimator:LoadAnimation(anim)
		KJUlt:Play()
	
	end)
end
coroutine.wrap(YNRLSZK_fake_script)()
